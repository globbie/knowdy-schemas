{schema knd-basic

   {!proc Sensory Process
          [gloss {ru {t сенсорный процесс}}]
      {is Agent Driven Obj Process}}

   {!proc Visual Process
          [gloss {ru {t процесс визуального восприятия}}]
      {is Sensory Process}
      [arg {Target     {cls Physical Matter}}
           {Instrument {cls Visual Tool}}  ]}

   {!proc Auditory Process
          [gloss {ru {t процесс слухового восприятия}}]
      {is Sensory Process} }

}