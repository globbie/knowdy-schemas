{schema knd-basic

{!proc Transportation Process
      [_gloss {ru {t процесс активного перемещения физического объекта}}]
      {is  Physical Impact Process}
      [arg {Obj       {_c Physical Matter}}
           {TargetLoc {_c Spatial Location}} ]
      [do {_move
             {obj      TObj}
             {where-to TargetLoc}} ]}

{!proc Throw
   {is Transportation Process}}

{!proc Throw #once
   {is Throw}}


{!proc Drop
   {is Transportation Process}}

}