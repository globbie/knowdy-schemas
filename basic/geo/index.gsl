{include city}

{schema knd-basic

{!class Spatial Object
        [gloss {ru {t пространственный объект}}]
  {is Abstraction}}

{!class Geographical Region
    [gloss {ru {t географический регион}}]
    {is Spatial Object}}

{!class Country
        [gloss {ru {t страна}}]
    {is Geographical Region}
    {ref repub {cls Republic} {t set}}
}

{!class Republic
        [gloss {ru {t республика}}]
  {is Geographical Region}
}

}
