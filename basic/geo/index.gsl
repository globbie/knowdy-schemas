{include city}

{schema knd-basic

{!cls Spatial Object
        [gloss {ru {t пространственный объект}}]
  {is Abstraction}}

{!cls Geographical Region
    [gloss {ru {t географический регион}}]
    {is Spatial Object}}

{!cls Country
        [gloss {ru {t страна}}]
    {is Geographical Region}
    {cls-ref repub {cls Republic} {t set}}
}

{!cls Republic
        [gloss {ru {t республика}}]
  {is Geographical Region}
}

}
