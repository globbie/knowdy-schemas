{schema knd-basic

{!class Bread
        [gloss {ru {t хлеб}}]
    {is Baked Food Product}}

{!proc Bread Making
       [gloss {ru {t приготовление хлеба}}]
   {is Baking}
   [effect {product  {cls Bread}}]
   [arg    {resource {cls Flour}}]}

{!class Rye Bread
        [gloss {ru {t ржаной хлеб}}]
    {is Bread
        {-- _proc Bread Making
               {_role effect}
               {resource Rye Flour} --} }}

}
