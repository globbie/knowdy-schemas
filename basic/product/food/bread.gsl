{schema knd-basic

{!class Bread
        [_gloss {ru {t хлеб}}]
    {is Baked Food Product}}

{!proc Bread Making
       [_gloss {ru {t приготовление хлеба}}]
   {is Baking}
   [effect {product {_c Bread}}]
   [arg {resource {_c Flour}}]}

{!class Rye Bread
        [_gloss {ru {t ржаной хлеб}}]
    {is Bread
        {-- _proc Bread Making
               {_role effect}
               {resource Rye Flour} --} }}

}
